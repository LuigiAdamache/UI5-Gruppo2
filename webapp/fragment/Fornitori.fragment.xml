<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="valueHelpDialog"
		title="{i18n>titleDialog}"
		items="{path: '/Suppliers'}"
		confirm="onValueHelpClose"
		cancel="onValueHelpClose"
        search="onValueHelpSearch" >
        
		<StandardListItem
			title="{SupplierID}"
			description="{CompanyName}" />
	</SelectDialog>
</core:FragmentDefinition>